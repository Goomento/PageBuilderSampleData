<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Goomento\PageBuilder\PageBuilder">
        <arguments>
            <argument name="components" xsi:type="array">
                <item name="pagebuilder.sample-data" xsi:type="string">Goomento\PageBuilderSampleData\EntryPoint</item>
            </argument>
        </arguments>
    </type>
    <type name="Goomento\PageBuilder\Model\LocalSampleCollection">
        <arguments>
            <argument name="samples" xsi:type="array">
                <item name="luma" xsi:type="string">Goomento\PageBuilderSampleData\Samples\Luma</item>
            </argument>
        </arguments>
    </type>
</config>

